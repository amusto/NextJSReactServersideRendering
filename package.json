    "express": "^4.16.4",
    "formik": "^1.4.2",
    "micro-form": "^2.0.5",
    "next": "^7.0.2",
    "nodemon": "^1.18.9",
    "react": "^16.6.3",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.6.3"
