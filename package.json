{
  "name": "nextjs-challenge-boilerplate",
  "version": "1.0.0",
  "description": "A basic boilerplate for nextjs challenge",
  "main": "index.js",
  "engines": {
    "node": "8.1.1",
    "npm": "5.0.3"
  },
  "jest": {
    "setupFiles": ["./setupTests.js"]
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "standard",
    "dev": "cross-env NODE_ENV=development npm start",
    "build": "next build",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install && npm run build",
    "start": "node ./server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remedyhealth/nextjs-challenge-boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/remedyhealth/nextjs-challenge-boilerplate/issues"
  },
  "homepage": "https://github.com/remedyhealth/nextjs-challenge-boilerplate#readme",
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "formik": "^1.4.2",
    "micro-form": "^2.0.5",
    "next": "^7.0.2",
    "nodemon": "^1.18.9",
    "react": "^16.6.3",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "acorn": "^6.0.5",
    "acorn-jsx": "^5.0.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.12.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-react": "^7.12.3",
    "espree": "^5.0.0",
    "jest": "^23.6.0",
    "standard": "^12.0.1"
  }
}
